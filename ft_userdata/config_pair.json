{
  "strategy": "PairBreakoutStrategy",
  "dry_run": true,
  "dry_run_wallet": 1000,
  "exchange": {
    "name": "binance",
    "pair_whitelist": ["ETH/USDT"],
    "pair_blacklist": ["BNB/USDT", ".*DOWN/.*"]
  },
  "timeframe": "1h",
  "max_open_trades": 1,
  "stake_currency": "USDT",
  "stake_amount": 100,
  "dataformat_ohlcv": "feather",
  "data_dir": "user_data/data/binance",
  "entry_pricing": {
    "price_side": "bid",
    "use_order_book": false,
    "order_book_depth": 1
  },
  "exit_pricing": {
    "price_side": "bid",
    "use_order_book": false,
    "order_book_depth": 1
  },
  "pairlists": [
    {
      "method": "StaticPairList",
      "pairs": ["ETH/USDT", "BTC/USDT"]
    }
  ]
}